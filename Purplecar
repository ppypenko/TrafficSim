package edu.csc150;

import greenfoot.Actor;

public class Purplecar extends Car25{
	
	public Purplecar(Direction r) {
		super(r);
		this.setImage("images/topCarPurple.png");
		setRotation(r.GetRotation());
		
	}
	public void act(){
		this.move(move);
		this.moveCar();
		this.checkIntersection();
		this.removeOrCrash();
	}
	@Override
	protected void turn(Actor i) {
		turnLeft(i);
	}
}
