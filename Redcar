package edu.csc150;

public class Redcar extends Car{

	static String img = "images/topCarRed.png";
	public Redcar(Direction r) {
		super(r);
		this.setImage(img);
		setRotation(r.GetRotation());
		COLOR = "RED";
	}
	public void act(){
		this.move(move);
		this.moveCar();
		this.newLocation();
		this.removeOrCrash();
	}
	public void removeOrCrash(){
		if(crash){
			exp++;
		}
		hasCrash();
	}
}


